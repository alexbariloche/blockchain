  "source": "pragma solidity >=0.4.21 <0.6.0;\n\ncontract CheckingSystem {\n\n\tstruct Persons {\n\t\tint id;\n\t\tstring name;\n\t}\n\n/*\tstruct Banks {\n\t\tint id;\n\t\tstring name;\n\t}\n\n\tstruct Accounts {\n\t\tint id;\n\t\tint bank_id;\n\t\tint owner_id;\n\t\tuint balance;\n\t}\n\n\tstruct Checks {\n\t\tint id;\n\t\tint bank_id;\n\t\tint owner_id;\n\t\tint account_id;\n\t\tuint amount;\n\t}\n*/\n\tmapping (int => Persons) public persons;\n/*\tmapping (int => Banks) public banks;\n\tmapping (int => Accounts) public accounts;\n\tmapping (int => Checks) public checks;\n\n*/\tint public personsCount = 0;\n/*\tint public banksCount = 0;\n\tint public accountsCount = 0;\n\tint public checksCount = 0;\n\n*/\tfunction addPerson (string memory _name) private {\n\t\tpersonsCount++;\n\t\tpersons[personsCount] = Persons( personsCount, _name);\n\t}\n\n/*\tfunction getPersonId(string storage _name) private {\n\t\tint id = -1;\n\t\tfor ( int p = 0; p < personsCount; ++p)\n\t\t{\n\t\t\tif (persons[p].name == _name)\n\t\t\t{\n\t\t\t\tid = persons[p].id;\n\t\t\t\tbreak;\n\t\t\t}\n\t\t}\n\t\treturn id;\n\t}\n\n\tfunction addBank (string memory _name) private {\n\t\tbanksCount++;\n\t\tbanks[banksCount] = Banks( banksCount, _name);\n\t}\n\n\tfunction addAccount (int _bank, int _owner) private {\n\t\taccountsCount++;\n\t\taccounts[accountsCount] = Accounts( accountsCount, _bank, _owner, 0);\n\t}\n\n\tfunction addCheck ( int _bank, int _owner,\n\t\t\t\t\t\tint _account, uint _amount) private {\n\t\tchecksCount++;\n\t\tchecks[checksCount] = Checks( checksCount, _bank, _owner, _account, _amount);\n\t}\n*/\n\tconstructor() public {\n\t\taddPerson(\"Alex\");\n\t\taddPerson(\"Fer\");\n/*\t\taddBank(\"Rio\");\n\t\taddAccount(0, 1);\n\t\taddCheck(0, 1, 0, 123);\n*/\t}\n}\n",
